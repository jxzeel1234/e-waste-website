{
  "analytics": {
    "enabled": true
  },
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/fonts/$1"
    }
  ]
} 